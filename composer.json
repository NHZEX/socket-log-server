{
    "name": "zxin/socket-log-server",
    "type": "project",
    "authors": [
        {
            "name": "auooru",
            "email": "auooru@outlook.com"
        }
    ],
    "minimum-stability": "stable",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SocketLog\\": "src/"
        },
        "files": [
            "src/func.php"
        ]
    },
    "require": {
        "php": "^8.0",
        "ext-ctype": "*",
        "ext-mbstring": "*",
        "ext-zlib": "*",
        "psr/log": "^1.1",
        "symfony/console": "^6.0",
        "vlucas/phpdotenv": "^5.5",
        "zxin/utils": "^2.4"
    },
    "require-dev": {
        "swoole/ide-helper": "^5"
    },
    "replace": {
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-mbstring": "*"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform-check": true,
        "platform": {
            "php": "8.1"
        }
    }
}
