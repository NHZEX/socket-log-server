{
    "name": "nhzex/socket-log-server",
    "type": "project",
    "authors": [
        {
            "name": "auooru",
            "email": "auooru@outlook.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Workerman\\Protocols\\": "protocols/"
        },
        "files": [
            "src/function.php"
        ]
    },
    "require": {
        "php": "^7.4|^8.0",
        "ext-zlib": "*",
        "symfony/polyfill-php80": "^1",
        "symfony/polyfill-php81": "^1",
        "workerman/workerman": "^4.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^5"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform-check": true
    }
}
